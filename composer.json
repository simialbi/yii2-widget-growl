{
    "name": "simialbi/yii2-widget-growl",
    "description": "A widget to generate growl based notifications",
    "keywords": [
        "yii2",
        "yii2-widgets",
        "extension",
        "widget",
        "growl",
        "alert",
        "notification",
        "bootstrap",
        "jquery",
        "plugin"
    ],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/simialbi/yii2-widget-growl/issues",
        "source": "https://github.com/simialbi/yii2-widget-growl"
    },
    "authors": [
        {
            "name": "Simon Karlen",
            "email": "simi.albi@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.3",
        "yiisoft/yii2": "^2.0.15",
        "simialbi/yii2-simialbi-base": ">=0.13.1 <1.0 | ^1.0.0",
        "npm-asset/bootstrap4-notify": "^4.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-coding-standards": "~2.0",
        "yiisoft/yii2-bootstrap4": "^2.0.0",
        "phpunit/phpunit": "^5.0.0"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap": "Bootstrap",
        "yiisoft/yii2-bootstrap4": "Bootstrap 4",
        "yiisoft/yii2-bootstrap5": "Bootstrap 5",
        "rmrevin/yii2-fontawesome": "Font awesome icons"
    },
    "config": {
        "process-timeout": 1800,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "autoload": {
        "psr-4": {
            "simialbi\\yii2\\growl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\extensions\\growl\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
